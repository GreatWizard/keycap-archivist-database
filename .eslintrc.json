{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": "error",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "no-console": "off",
    "guard-for-in": "off",
    "no-nested-ternary": "off",
    "object-curly-newline": "off"
  }
}
